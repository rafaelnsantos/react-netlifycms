{
  "name": "react-netlifycms",
  "version": "1.0.3",
  "author": "Rafael Santos",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafaelnsantos/react-netlifycms.git"
  },
  "bugs": {
    "url": "https://github.com/rafaelnsantos/react-netlifycms/issues"
  },
  "homepage": "https://github.com/rafaelnsantos/react-netlifycms#readme",
  "scripts": {
    "commit": "git-cz",
    "prebuild": "rimraf lib",
    "build": "yarn build:esm && yarn build:cjs && yarn build:umd",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "build:umd": "webpack",
    "release": "semantic-release",
    "test": "yarn build"
  },
  "dependencies": {
    "netlify-cms-app": "^2.15.39",
    "netlify-identity-widget": "^1.9.2"
  },
  "main": "./lib/umd/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@monx/eslint-config-next": "^1.0.4",
    "@monx/versioning": "^1.0.1",
    "@types/netlify-identity-widget": "^1.9.2",
    "@types/node": "^14.0.4",
    "@types/react": "^16.9.35",
    "awesome-typescript-loader": "^5.2.1",
    "eslint": "^7.3.1",
    "husky": "^7.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5",
    "webpack": "^5.51.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
